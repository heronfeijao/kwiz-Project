<% if (user) { %>
  <% for (const card of db) { %>
    <% if (card.public) { %>
      <article class="col-lg-4 col-md-5 col-10">
        <div class="card kwizcard">
          <img src="<%= card.imageurl %>" class="card-img-top img-fluid" alt="quizimg">
          <div class="card-body">
            <h5 class="card-title">
              <%= card.title %> KWIZ
            </h5>
            <p class="card-text">
              <%= card.description %>
            </p>
            <a href="/kwiz/<%= card.id %>" class="btn btn-primary kwizmain">KWIZ!</a>
          </div>
        </div>
      </article>
      <% } %>
        <% } %>
          <% } %>
